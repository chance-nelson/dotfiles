lalt - e : yabai -m space --layout bsp lalt - s : yabai -m space --layout stack

lalt - down : yabai -m window --focus stack.next || yabai -m window --focus south
lalt - up : yabai -m window --focus stack.prev || yabai -m window --focus north
lalt - left : yabai -m window --focus west
lalt - right : yabai -m window --focus east

lalt - 1 : yabai -m space --focus 1
lalt - 2 : yabai -m space --focus 2
lalt - 3 : yabai -m space --focus 3
lalt - 4 : yabai -m space --focus 4
lalt - 5 : yabai -m space --focus 5
lalt - 6 : yabai -m space --focus 6

lalt + shift - 1 : yabai -m window --space 1
lalt + shift - 2 : yabai -m window --space 2
lalt + shift - 3 : yabai -m window --space 3
lalt + shift - 4 : yabai -m window --space 4
lalt + shift - 5 : yabai -m window --space 5
lalt + shift - 6 : yabai -m window --space 6

lalt - space : yabai -m window --toggle float

ctrl + lalt - left : yabai -m window --resize left:-100:0 ; yabai -m window --resize right:-100:0
ctrl + lalt - down : yabai -m window --resize bottom:0:100 ; yabai -m window --resize top:0:100
ctrl + lalt - up : yabai -m window --resize top:0:-100 ; yabai -m window --resize bottom:0:-100
ctrl + lalt - right : yabai -m window --resize right:100:0 ; yabai -m window --resize left:100:0

shift + lalt - left : yabai -m window --warp west || yabai -m window --toggle split
shift + lalt - right : yabai -m window --warp east || yabai -m window --toggle split
shift + lalt - up : yabai -m window --warp north || yabai -m window --toggle split
shift + lalt - down : yabai -m window --warp south || yabai -m window --toggle split

lalt - q : skhd --key "cmd - w"
lalt - return : open -n /Applications/iTerm.app
lalt - w : open -n /Applications/Google\ Chrome.app
